{
  "assistant": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-4o"
    },
    "version": "2"
  },
  "vim_mode": true,
  "ui_font_size": 15,
  "ui_font_family": "mononoki",
  "buffer_font_size": 16,
  "buffer_font_family": "mononoki",
  "relative_line_numbers": true,
  "remove_trailing_whitespace_on_save": true,
  "theme": {
    "mode": "system",
    "light": "Gruvbox Dark Hard",
    "dark": "Gruvbox Dark Hard"
  }
}
